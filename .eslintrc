{
  "extends": ["airbnb", "prettier"],
  "env": {
    "es6": true,
    "browser": true,
    "jquery": true
  },
  "rules": {
    "no-console": 0,
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": true
      }
    ],
    "rules": {
      "prettier/prettier": "error",
      "no-console": 0
    }
  },
  "plugins": ["html", "prettier"]
}
